{
  "name": "dirac-lang",
  "version": "0.1.0",
  "description": "LLM-Augmented Declarative Execution",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dirac": "dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsup src/index.ts src/cli.ts --format esm --dts --clean",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "declarative",
    "dirac",
    "bra-ket"
  ],
  "author": "Zhi Wang",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^4.3.5",
    "openai": "^6.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
